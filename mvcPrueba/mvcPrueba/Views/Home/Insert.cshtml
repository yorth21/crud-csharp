@model PruebaVewModel

@{
    <form action='@Url.Action("Insert", "Home")' method="post">
        <input name"Id" type="hidden" value="@Model.Id" />
        <input name="Nombre" value="@Model.Nombre" />
        <input name="Marca" value="@Model.Marca" />
        <input name="Alcohol" value="@Model.Alcohol" />
        <input name="Cantidad" value="@Model.Cantidad" />
        <input type="submit" class="btn btn-primary" value="Insertar" />
    </form>
}
